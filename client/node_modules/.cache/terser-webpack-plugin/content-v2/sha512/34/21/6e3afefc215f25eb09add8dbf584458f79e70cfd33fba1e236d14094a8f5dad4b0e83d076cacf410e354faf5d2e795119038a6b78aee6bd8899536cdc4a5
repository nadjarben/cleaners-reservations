' />\\r\\n      </div>\\r\\n      <Dialog fullScreen open={open} onClose={handleClose} TransitionComponent={Transition}>\\r\\n        <AppBar className={classes.appBar}>\\r\\n          <Toolbar>\\r\\n            <IconButton edge=\\\"start\\\" color=\\\"inherit\\\" onClick={handleClose} aria-label=\\\"Close\\\">\\r\\n              <CloseIcon />\\r\\n            </IconButton>\\r\\n            <Typography variant=\\\"h6\\\" className={classes.title}>\\r\\n              Commander une livraison\\r\\n            </Typography>\\r\\n          </Toolbar>\\r\\n        </AppBar>\\r\\n        <div className='container'>\\r\\n        <TabModal />\\r\\n        </div>\\r\\n      </Dialog>\\r\\n    </div>\\r\\n  );\\r\\n}\\r\\n\",\"import React, { Component } from 'react';\\r\\nimport { Form, Button } from 'react-bootstrap';\\r\\nimport PropTypes from \\\"prop-types\\\";\\r\\nimport { connect } from \\\"react-redux\\\";\\r\\nimport { postCustomer } from '../../../store/actions/customerActions';\\r\\nimport { FormattedMessage } from 'react-intl'; \\r\\n\\r\\nclass FormNewCustomer extends Component {\\r\\n    state = {\\r\\n        name: '',\\r\\n        surname: '', \\r\\n        phone: '', \\r\\n        email: '', \\r\\n        address: '',  \\r\\n        info: ''\\r\\n      };\\r\\n\\r\\n    handleChange = (e) => {\\r\\n    this.setState ({[e.target.name]: e.target.value})\\r\\n    }\\r\\n    \\r\\n  handleSubmit = (e) => {\\r\\n    const { name, surname, phone, email, address, info } = this.state\\r\\n    this.props.postCustomer(name, surname, phone, email, address, info)\\r\\n  }\\r\\n\\r\\n    render() {\\r\\n        return(\\r\\n          <div style={{marginTop:'5%'}} className='container'>\\r\\n            \\r\\n            <Form onSubmit={e => this.handleSubmit(e)} className=\\\"form\\\" >\\r\\n  <Form.Row>\\r\\n    <Form.Group controlId=\\\"formGridFirstName\\\" >\\r\\n      <Form.Control required={true} type=\\\"text\\\" placeholder=\\\"First Name\\\" name=\\\"name\\\" val